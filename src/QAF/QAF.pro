TARGET = QAF
win32:DESTDIR = ../../bin/win32
unix:DESTDIR = ../../bin/unix
QT += core widgets gui
CONFIG += debug_and_release console
DEFINES += QT_DLL QT_WIDGETS_LIB
INCLUDEPATH += ./GeneratedFiles \
    . \
    ./GeneratedFiles/Debug \
    ./../QAFCore \
    ./../QtAwesome \
    ./../Utils \
    ./../../include
