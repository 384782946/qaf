DEPENDPATH += .
CONFIG(debug,debug|release){
    win32:TARGET=$$join(TARGET,,,d)
    mac:TARGET=$$join(TARGET,,,_debug)

    LIBS += -lQAFCored \
        -llog4qtd \
        -lQtAwesomed
}

CONFIG(release,release|debug){
    LIBS += -lQAFCore \
        -llog4qt \
        -lQtAwesome
}
include(QAF.pri)
win32:RC_FILE = QAF.rc
